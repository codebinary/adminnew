<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Testdrive" table="testdrive">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="string" column="nombre" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="apepaterno" type="string" column="apePaterno" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="apematerno" type="string" column="apeMaterno" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dniextranjeria" type="string" column="dniExtranjeria" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="distrito" type="string" column="distrito" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="empresa" type="string" column="empresa" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cargo" type="string" column="cargo" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="celular" type="string" column="celular" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="modelo" type="string" column="modelo" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="formacontacto" type="string" column="formaContacto" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="comentario" type="text" column="comentario" length="65535" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="newsletter" type="string" column="newsletter" length="3" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="politicas" type="string" column="politicas" length="3" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="fecha" type="datetime" column="fecha" nullable="false">
      <options>
        <option name="default">CURRENT_TIMESTAMP</option>
      </options>
    </field>
  </entity>
</doctrine-mapping>
